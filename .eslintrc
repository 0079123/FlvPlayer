{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-bitwise": 0,
        "no-console": 0,
        "no-param-reassign": 0,
        "import/no-named-as-default-member": 0
    },
    "parserOptions": {
        "sourceType": "module"
    }
}