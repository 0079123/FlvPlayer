{
    "name": "flvplayer",
    "version": "1.0.7",
    "description": "FlvPlayer is a JavaScript player for decode flv to the canvas",
    "main": "dist/flvplayer.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development rollup -c -m -w",
        "build": "cross-env NODE_ENV=production rollup -c",
        "server": "npx cross-env NODE_ENV=development && node ./server/index.js",
        "start": "npx cross-env NODE_ENV=development && npx servor docs index.html 8080 5000",
        "test": "eslint src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zhw2590582/FlvPlayer.git"
    },
    "keywords": [
        "flv",
        "video",
        "canvas",
        "player",
        "h264",
        "aac"
    ],
    "author": "Harvey Zack <laozhaochaguan@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zhw2590582/FlvPlayer/issues"
    },
    "homepage": "https://github.com/zhw2590582/FlvPlayer#readme",
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/plugin-external-helpers": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/runtime": "^7.4.4",
        "autoprefixer": "^9.5.1",
        "babel-eslint": "^10.0.1",
        "cross-env": "^5.2.0",
        "cssnano": "^4.1.10",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.2.0",
        "eslint-plugin-import": "^2.17.2",
        "node-media-server": "^2.1.2",
        "node-sass": "^4.12.0",
        "rollup": "^1.11.3",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-eslint": "^5.1.0",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-uglify": "^6.0.2",
        "servor": "^2.2.1"
    },
    "dependencies": {
        "screenfull": "^4.2.0",
        "yuv-buffer": "^1.0.0",
        "yuv-canvas": "^1.2.6"
    }
}
